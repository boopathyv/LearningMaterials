<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="/assets/styles.css" />
</head>
<body>
    <% include partials/nav.ejs %>
    <h1><%= qs.id %></h1>
    <input id='name' label='Name' value=<%= qs.id %>><%= qs.id %></input>
    <input id='empid' label='Emp ID'></input>

    <h1>HOBBIES</h1>
    <% data.hobbies.forEach(function(item){ %>
        <h2><%= item%></h2>
    <% }); %>


    <form id='my-form' method='POST' action='/profile'>
    <label for="who">Who</label>
    <input type='text' name="who" value='<%= qs.person%>'>

    <label for="dept">Department</label>
    <input type='text' name="dept" value='<%= qs.dept%>'>

    <label for="email">Email</label>
    <input type='text' name="email" value='<%= qs.email%>'>
    <input type='submit' name="submit">
    </form>
</body>
</html>